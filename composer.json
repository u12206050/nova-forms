{
    "name": "day4/nova-forms",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova",
        "form"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "day4/nova-translatable-resource": "^3.1",
        "day4/switch-locale": ">=0.5",
        "graham-campbell/throttle": "^8.0",
        "mll-lab/graphql-php-scalars": "^4.0",
        "whitecube/nova-flexible-content": "^0.2"
    },
    "autoload": {
        "psr-4": {
            "Day4\\NovaForms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Day4\\NovaForms\\NovaFormsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
